package four_pillow;

class CappuccinoMaker extends CoffeeMaker {

	public CappuccinoMaker() {
		this.maxVolumeOz = 16;
		this.milk = "whole";
	}
	
	public void brew(String beans) {
		super.brew(beans);
		System.out.println("Frothy!!!");
	}
	
	public void clean() {
		System.out.println("Clearning");
	}
	
}
